<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo2">

    <Button
        android:id="@+id/buttonMapa"
        android:layout_width="75dp"
        android:layout_height="77dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="296dp"
        android:layout_marginBottom="262dp"
        android:background="@drawable/maps"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtMenu"
        android:layout_width="147dp"
        android:layout_height="80dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="609dp"
        android:background="@drawable/menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="361dp"
        android:layout_height="174dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonMapa"
        app:srcCompat="@drawable/adminiscar_mobile04" />

</androidx.constraintlayout.widget.ConstraintLayout>