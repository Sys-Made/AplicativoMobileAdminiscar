<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ActivityTelaMenu">

        <Button
            android:id="@+id/buttonServicos"
            android:layout_width="75dp"
            android:layout_height="69dp"
            android:layout_marginTop="114dp"
            android:layout_marginEnd="38dp"
            android:layout_marginBottom="98dp"
            app:layout_constraintBottom_toTopOf="@+id/buttonSobre"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/buttonSocio"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txtMenu"
            android:layout_width="149dp"
            android:layout_height="57dp"
            android:layout_marginStart="175dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="179dp"
            android:layout_marginBottom="118dp"
            android:fontFamily="sans-serif-black"
            android:includeFontPadding="false"
            android:textAlignment="center"
            android:textColor="@android:color/background_light"
            android:textSize="40sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonCliente"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.478"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonSair"
            android:layout_width="67dp"
            android:layout_height="62dp"
            android:layout_marginStart="172dp"
            android:layout_marginEnd="172dp"
            android:layout_marginBottom="17dp"
            android:padding="5dp"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonCliente" />

        <Button
            android:id="@+id/buttonSocio"
            android:layout_width="71dp"
            android:layout_height="64dp"
            android:layout_marginStart="37dp"
            android:layout_marginTop="114dp"
            android:layout_marginEnd="193dp"
            android:layout_marginBottom="102dp"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonMapa"
            app:layout_constraintEnd_toStartOf="@+id/buttonServicos"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonCliente"
            android:layout_width="74dp"
            android:layout_height="67dp"
            android:layout_marginStart="171dp"
            android:layout_marginEnd="171dp"
            android:layout_marginBottom="273dp"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/buttonSair"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtMenu" />

        <Button
            android:id="@+id/buttonMapa"
            android:layout_width="73dp"
            android:layout_height="74dp"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="195dp"
            android:layout_marginBottom="262dp"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/buttonSobre"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonSocio" />

        <Button
            android:id="@+id/buttonSobre"
            android:layout_width="75dp"
            android:layout_height="72dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="262dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/buttonMapa"
            app:layout_constraintTop_toBottomOf="@+id/buttonServicos" />

        <Button
            android:id="@+id/buttonContrato"
            android:layout_width="60dp"
            android:layout_height="72dp"
            android:layout_marginStart="175dp"
            android:layout_marginTop="118dp"
            android:layout_marginEnd="176dp"
            android:layout_marginBottom="83dp"
            app:layout_constraintBottom_toTopOf="@+id/buttonSair"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonCliente" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>